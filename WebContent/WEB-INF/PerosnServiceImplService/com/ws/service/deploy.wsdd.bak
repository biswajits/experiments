<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from PerosnServiceImplService WSDL service -->

  <service name="PerosnServiceImpl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service.ws.com"/>
      <parameter name="wsdlServiceElement" value="PerosnServiceImplService"/>
      <parameter name="schemaQualified" value="http://model.ws.com,http://service.ws.com"/>
      <parameter name="wsdlServicePort" value="PerosnServiceImpl"/>
      <parameter name="className" value="com.ws.service.PerosnServiceImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="PerosnServiceImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="searchPerson" qname="operNS:searchPerson" xmlns:operNS="http://service.ws.com" returnQName="retNS:searchPersonReturn" xmlns:retNS="http://service.ws.com" returnType="rtns:Person" xmlns:rtns="http://model.ws.com" soapAction="" >
        <parameter qname="pns:per" xmlns:pns="http://service.ws.com" type="tns:Person" xmlns:tns="http://model.ws.com"/>
      </operation>
      <parameter name="allowedMethods" value="searchPerson"/>

      <typeMapping
        xmlns:ns="http://model.ws.com"
        qname="ns:Person"
        type="java:com.ws.model.Person"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
